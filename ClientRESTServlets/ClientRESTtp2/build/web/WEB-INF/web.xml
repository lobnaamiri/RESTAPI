<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>EnregistrementServlet</servlet-name>
        <servlet-class>controler.EnregistrementServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>controler.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConnectionServlet</servlet-name>
        <servlet-class>controler.ConnectionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreerProjetServlet</servlet-name>
        <servlet-class>controler.CreerProjetServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AfficherProjetsServlet</servlet-name>
        <servlet-class>controler.AfficherProjetsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AfficherDetailsProjetsServlet</servlet-name>
        <servlet-class>controler.AfficherDetailsProjetsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AfficherInvitationsServlet</servlet-name>
        <servlet-class>controler.AfficherInvitationsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnregistrerReponseInvitationServlet</servlet-name>
        <servlet-class>controler.EnregistrerReponseInvitationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SupprimerMembreServlet</servlet-name>
        <servlet-class>controler.SupprimerMembreServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnvoyerInvitationServlet</servlet-name>
        <servlet-class>controler.EnvoyerInvitationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EnregistrementServlet</servlet-name>
        <url-pattern>/EnregistrementServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConnectionServlet</servlet-name>
        <url-pattern>/ConnectionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreerProjetServlet</servlet-name>
        <url-pattern>/CreerProjetServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AfficherProjetsServlet</servlet-name>
        <url-pattern>/AfficherProjetsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AfficherDetailsProjetsServlet</servlet-name>
        <url-pattern>/AfficherDetailsProjetsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AfficherInvitationsServlet</servlet-name>
        <url-pattern>/AfficherInvitationsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnregistrerReponseInvitationServlet</servlet-name>
        <url-pattern>/EnregistrerReponseInvitationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SupprimerMembreServlet</servlet-name>
        <url-pattern>/SupprimerMembreServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnvoyerInvitationServlet</servlet-name>
        <url-pattern>/EnvoyerInvitationServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
